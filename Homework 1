

import collections

def create_sets(string1, string2):
    set1 = set(string1)
    set2 = set(string2)
    return set1, set2

def create_multisets(string1, string2):
    multiset1 = collections.Counter(string1)
    multiset2 = collections.Counter(string2)
    return multiset1, multiset2

def main():
    string1 = input("Enter the first string: ")
    string2 = input("Enter the second string: ")

    set1, set2 = create_sets(string1, string2)

    print("Sets:")
    print("Set 1:", set1)
    print("Set 2:", set2)

    print("Union of sets:", set1.union(set2))
    print("Intersection of sets:", set1.intersection(set2))

    multiset1, multiset2 = create_multisets(string1, string2)

    print("\nMultisets:")
    print("Multiset 1:", multiset1)
    print("Multiset 2:", multiset2)

    print("Union of multisets:", multiset1 + multiset2)
    print("Intersection of multisets:", multiset1 & multiset2)

if __name__ == "__main__":
    main()
